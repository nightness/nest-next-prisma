{
  "name": "your-app",
  "version": "0.0.0",
  "description": "This template is a tight coupling of Next.js, Nest, Prisma ORM, and Docker in to a single project using a hybrid server, with an optional Docker development container that supports live updates. This creates a streamlined developer experience. Both Next and Nest can also be run separately.",
  "author": "your-name",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "node system/scripts/setup-env.mjs",
    "postinstall": "npx prisma generate",
    "dev": "node system/scripts/start-hybrid.js --watch",
    "dev:nest": "npx nest start --watch",
    "dev:next": "npx next dev",
    "build": "rm -rf .nest .next && nest build -p tsconfig.build.json && next build",
    "build:nest": "npx nest build -p tsconfig.build.json",
    "build:next": "npx next build",
    "start": "node system/scripts/start-hybrid.js",
    "start:nest": "npx nest start -p tsconfig.build.json",
    "start:next": "npx next start",
    "clean": "rm -rf .next .nest",
    "purge": "rm -rf node_modules .next .nest yarn.lock package-lock.json pnpm-lock.yaml",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md}\"",
    "prisma:config": "node system/scripts/prisma-config.js",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "ts-node --transpile-only prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build -t your-app .",
    "docker:migrate": "docker exec -it $(docker ps -qf \"name=your-app\") npm run migrate",
    "docker:up": "docker-compose up -d",
    "docker:start": "docker-compose up --build",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:down": "docker-compose down && docker-compose -f docker-compose.dev.yml down",
    "docker:shell": "docker exec -it your-app-1 /bin/sh",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "^10.4.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.20.0",
    "@react-spring/web": "^9.7.4",
    "@tabler/icons-react": "^3.19.0",
    "apexcharts": "^3.54.0",
    "babel-loader": "^9.2.1",
    "bcryptjs": "^2.4.3",
    "chokidar": "^3.5.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "is-docker": "^3.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "next": "^14.2.15",
    "nodemailer": "^6.9.15",
    "prisma": "^5.20.0",
    "react": "^18.3.1",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@eslint/js": "^9.10.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.17.9",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.16",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
